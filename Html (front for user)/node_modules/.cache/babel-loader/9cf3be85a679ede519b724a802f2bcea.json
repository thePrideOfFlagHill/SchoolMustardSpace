;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,8BAAA,OAAA,EADA;;AAAA;AAAA;AACA,gBAAA,IADA,qBACA,IADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA;AAjBA,GAZA;AA+BA,EAAA,OA/BA,qBA+BA;AACA,SAAA,OAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,SAAA,cAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA;AAlCA,CAAA","sourcesContent":["<template>\r\n  <d2-container>\r\n    <div class=\"detail\">\r\n      <div>\r\n        <img src alt />\r\n        <span class=\"username\">{{userInfo.name}}</span>\r\n        <span class=\"end-time\">截止于 {{workInfo.end_time}}</span>\r\n      </div>\r\n      <!-- <span class=\"lable\">其他</span> -->\r\n      <div class=\"title\">{{workInfo.title}}</div>\r\n      <p class=\"content\">{{workInfo.content}}</p>\r\n      <img class=\"img\" v-lazy=\"`${workInfo.image}?param=200y200`\" alt />\r\n      <div style=\"height:20px\">\r\n        <span class=\"start-time d2-fr\">发布于{{workInfo.start_time}}</span>\r\n      </div>\r\n    </div>\r\n    <div class=\"tcc\">\r\n      <p>\r\n        <i class=\"fa fa-thumbs-o-up\" aria-hidden=\"true\"></i>\r\n        <span v-if=\"workInfo.thumbUp != undefined\">{{workInfo.thumbUp}}</span>\r\n        <span v-else>{{workInfo.thumb_up}}</span>\r\n      </p>\r\n      <p>\r\n        <i class=\"el-icon-star-off\"></i>\r\n        <span>{{workInfo.collect}}</span>\r\n      </p>\r\n      <p>\r\n        <i class=\"fa fa-comment-o itop\" aria-hidden=\"true\"></i>\r\n        <span>{{workInfo.comment}}</span>\r\n      </p>\r\n    </div>\r\n    <comment-list :data=\"commentInfo\"></comment-list>\r\n  </d2-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport CommentList from \"@/components/commentList/commentList\";\r\nexport default {\r\n  name: \"page1\",\r\n  data() {\r\n    return {\r\n      workInfo: \"\",\r\n      userInfo: \"\",\r\n      commentInfo:\"\"\r\n    };\r\n  },\r\n  components: {\r\n    CommentList\r\n  },\r\n  methods: {\r\n    async getinfo(item) {\r\n      // console.log(item);\r\n      this.workInfo = item;\r\n      const { data } = await axios.get(`/api/user/query/id/${item.user_id}`);\r\n      this.userInfo = data.data;\r\n      // console.log(this.userInfo);\r\n    },\r\n    async getCommentInfo(item) {\r\n      const {data} = await axios.get(`/api/query/task/comment/table_id/${item.id}`)\r\n      // console.log(data);\r\n      this.commentInfo = data.data;\r\n      // this.$options.methods.getUserName(this.commentInfo.user_id);\r\n    },\r\n    async getUserName(user_id) {\r\n      const { data } = await axios.get(`/api/user/query/id/${user_id}`);\r\n      console.log(data);\r\n    }\r\n  },\r\n  created() {\r\n    this.getinfo(this.$route.params.data);\r\n    this.getCommentInfo(this.$route.params.data);\r\n  }\r\n};\r\n</script>\r\n<style lang = \"less\" scoped>\r\n.detail {\r\n  border: 2px solid #e6e6e6;\r\n  height: auto;\r\n  .username {\r\n    font-size: 20px;\r\n  }\r\n  .end-time {\r\n    font-size: 15px;\r\n    color: gray;\r\n    margin-left: 20px;\r\n  }\r\n  .lable {\r\n    border: 1px solid #e6e6e6;\r\n    font-size: 15px;\r\n    padding: 2px 4px;\r\n    margin-top: 10px;\r\n    display: inline-block;\r\n  }\r\n  .title {\r\n    font-size: 30px;\r\n  }\r\n  .content {\r\n    color: #656565;\r\n  }\r\n  .img {\r\n    /* width: 300px; */\r\n    display: block;\r\n    height: 300px;\r\n  }\r\n  .start-time {\r\n    /* border: 1px solid red; */\r\n    font-size: 15px;\r\n    color: gray;\r\n    /* position: absolute; */\r\n    /* left: 80%; */\r\n  }\r\n  \r\n}\r\n.tcc {\r\n       margin-top: -10px;\r\n        p{\r\n            padding: 0;\r\n            display: inline-block;\r\n            margin-right: 20px;\r\n        }\r\n        span{\r\n            margin-left: 5px;\r\n            color: black;\r\n        }\r\n        flex-direction: row;\r\n        }\r\n        .itop{\r\n            display: inline-block;\r\n            margin-bottom: 5px;\r\n        }\r\n</style>"],"sourceRoot":"src/views/work"}]}