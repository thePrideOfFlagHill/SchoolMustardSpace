SAAKF,MAAL,CAAYC,MAAZ,CAAmB,8BAAnB,EAFS,CAGT;;AACA,SAAKD,MAAL,CAAYG,QAAZ,CAAqB,sBAArB,EAJS,CAKT;;AACA,SAAKH,MAAL,CAAYC,MAAZ,CAAmB,gBAAnB,EANS,CAOT;;AACA,SAAKD,MAAL,CAAYG,QAAZ,CAAqB,2BAArB;AACD;AAxBK,CAAR,EAyBGC,MAzBH,CAyBU,MAzBV","sourcesContent":["// Vue\nimport Vue from 'vue'\nimport i18n from './i18n'\nimport App from './App'\n// 核心插件\nimport d2Admin from '@/plugin/d2admin'\n// store\nimport store from '@/store/index'\nimport axios from 'axios'\n// 菜单和路由设置\nimport router from './router'\nimport { menuHeader, menuAside } from '@/menu'\nimport { frameInRoutes } from '@/router/routes'\nimport VueLazyload from 'vue-lazyload'\n\n// 核心插件\nVue.use(d2Admin)\naxios.defaults.baseURL = 'http://47.100.251.3/space/'\nVue.use(VueLazyload, {\n  loading: 'assets/img/load.gif'\n})\nnew Vue({\n  router,\n  store,\n  i18n,\n  render: h => h(App),\n  created () {\n    // 处理路由 得到每一级的路由设置\n    this.$store.commit('d2admin/page/init', frameInRoutes)\n    // 设置顶栏菜单\n    this.$store.commit('d2admin/menu/headerSet', menuHeader)\n    // 设置侧边栏菜单\n    this.$store.commit('d2admin/menu/asideSet', menuAside)\n    // 初始化菜单搜索功能\n    this.$store.commit('d2admin/search/init', menuHeader)\n  },\n  mounted () {\n    // 展示系统信息\n    this.$store.commit('d2admin/releases/versionShow')\n    // 用户登录后从数据库加载一系列的设置\n    this.$store.dispatch('d2admin/account/load')\n    // 获取并记录用户 UA\n    this.$store.commit('d2admin/ua/get')\n    // 初始化全屏监听\n    this.$store.dispatch('d2admin/fullscreen/listen')\n  }\n}).$mount('#app')\n"]}]}