AAA,IADA;AAEA,UAAA,OAAA,EAAA,OAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA,CARA;AAeA,QAAA,IAAA,EAAA,CACA;AACA,UAAA,QAAA,EAAA,IADA;AAEA,UAAA,OAAA,EAAA,QAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA;AAfA;AA/BA,KAAA;AAuDA,GA1DA;AA2DA,EAAA,OA3DA,qBA2DA;AAAA;;AACA,SAAA,YAAA,GAAA,WAAA,CAAA,YAAA;AACA,MAAA,KAAA,CAAA,WAAA;AACA,KAFA,EAEA,IAFA,CAAA;AAGA,GA/DA;AAgEA,EAAA,QAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA;AAHA,GAhEA;AAqEA,EAAA,aArEA,2BAqEA;AACA,IAAA,aAAA,CAAA,KAAA,YAAA,CAAA;AACA,GAvEA;AAwEA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,IAAA,GAAA,KAAA,GAAA,MAAA,CAAA,UAAA,CAAA;AACA,KAHA;;AAIA;;;;AAIA,IAAA,kBARA,8BAQA,IARA,EAQA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AACA,WAAA,MAAA;AACA,KAZA;;AAaA;;;AAGA;AACA,IAAA,MAjBA,oBAiBA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,SAAA;AACA,UAAA,KAAA,CACA,IADA,CAEA,mCACA,MAAA,CAAA,SAAA,CAAA,QADA,GAEA,YAFA,GAGA,MAAA,CAAA,SAAA,CAAA,QALA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,gBAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,kBAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AACA,cAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,kBAAA,EAAA;AACA,gBAAA,IAAA,EAAA,MAAA,CAAA,SAAA,CAAA;AADA,eAAA;;AAGA,cAAA,KAAA,CACA,GADA,CACA,mCAAA,MAAA,CAAA,IADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,qBAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,eAJA;;AAKA,cAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QAAA;;AAEA,cAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA;AAAA,eAAA;AACA,aAbA,MAaA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA,WAzBA,EA0BA,KA1BA,CA0BA,UAAA,KAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,eAAA;AACA,WA5BA;AA6BA,SA/BA,MA+BA;AACA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA,OApCA;AAqCA,KAvDA;AAwDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,IAAA,KArEA,mBAqEA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,6BACA,KAAA,CAAA,GAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,aAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,SAAA,CAAA;AAFA,iBAAA,CADA,EACA,IADA,cACA,IADA;AAMA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA;AA5EA;AAxEA,CAAA","sourcesContent":["<template>\n  <div class=\"page-login\">\n    <div class=\"page-login--layer page-login--layer-area\">\n      <ul class=\"circles\">\n        <li v-for=\"n in 10\" :key=\"n\"></li>\n      </ul>\n    </div>\n    <div class=\"page-login--layer page-login--layer-time\" flex=\"main:center cross:center\">{{time}}</div>\n    <div class=\"page-login--layer\">\n      <div class=\"page-login--content\" flex=\"dir:top main:justify cross:stretch box:justify\">\n        <div class=\"page-login--content-header\">\n          <p class=\"page-login--content-header-motto\">时间是一切财富中最宝贵的财富</p>\n        </div>\n        <div class=\"page-login--content-main\" flex=\"dir:top main:center cross:center\">\n          <!-- logo -->\n          <img class=\"page-login--logo\" src=\"./image/logo@2x.png\" />\n          <!-- form -->\n          <div class=\"page-login--form\">\n            <el-card shadow=\"never\">\n              <el-form\n                ref=\"loginForm\"\n                label-position=\"top\"\n                :rules=\"rules\"\n                :model=\"formLogin\"\n                size=\"default\"\n              >\n                <el-form-item prop=\"username\">\n                  <el-input type=\"text\" v-model=\"formLogin.username\" placeholder=\"用户名\">\n                    <i slot=\"prepend\" class=\"fa fa-user-circle-o\"></i>\n                  </el-input>\n                </el-form-item>\n                <el-form-item prop=\"password\">\n                  <el-input type=\"password\" v-model=\"formLogin.password\" placeholder=\"密码\">\n                    <i slot=\"prepend\" class=\"fa fa-keyboard-o\"></i>\n                  </el-input>\n                </el-form-item>\n                <el-form-item prop=\"code\">\n                  <el-input type=\"text\" v-model=\"formLogin.code\" placeholder=\"验证码\">\n                    <template slot=\"append\">\n                      <img class=\"login-code\" src=\"./image/login-code.png\" />\n                    </template>\n                  </el-input>\n                </el-form-item>\n                <el-button size=\"default\" @click=\"submit\" type=\"primary\" class=\"button-login\">登录</el-button>\n              </el-form>\n            </el-card>\n            <p class=\"page-login--options\" flex=\"main:justify cross:center\">\n              <span>\n                <d2-icon name=\"question-circle\" />忘记密码\n              </span>\n              <span>注册用户</span>\n            </p>\n            <!-- quick login -->\n            <el-button\n              class=\"page-login--quick\"\n              size=\"default\"\n              type=\"info\"\n              @click=\"dialogVisible = true\"\n            >快速选择用户（测试功能）</el-button>\n          </div>\n        </div>\n        <div class=\"page-login--content-footer\">\n          <p class=\"page-login--content-footer-locales\">\n            <a\n              v-for=\"language in $languages\"\n              :key=\"language.value\"\n              @click=\"onChangeLocale(language.value)\"\n            ></a>\n          </p>\n        </div>\n      </div>\n    </div>\n    <el-dialog title=\"快速选择用户\" :visible.sync=\"dialogVisible\" width=\"400px\">\n      <el-row :gutter=\"10\" style=\"margin: -20px 0px -10px 0px;\">\n        <el-col v-for=\"(user, index) in users\" :key=\"index\" :span=\"8\">\n          <div class=\"page-login--quick-user\" @click=\"handleUserBtnClick(user)\">\n            <d2-icon name=\"user-circle-o\" />\n            <span>{{user.name}}</span>\n          </div>\n        </el-col>\n      </el-row>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport dayjs from \"dayjs\";\nimport { mapActions } from \"vuex\";\nimport localeMixin from \"@/locales/mixin.js\";\nimport axios from \"axios\";\nexport default {\n  mixins: [localeMixin],\n  data() {\n    return {\n      userid: \"\",\n\n      timeInterval: null,\n      time: dayjs().format(\"HH:mm:ss\"),\n      // 快速选择用户\n      dialogVisible: false,\n      users: [\n        {\n          name: \"Admin\",\n          username: \"admin\",\n          password: \"admin\"\n        },\n        {\n          name: \"Editor\",\n          username: \"editor\",\n          password: \"editor\"\n        },\n        {\n          name: \"User1\",\n          username: \"user1\",\n          password: \"user1\"\n        }\n      ],\n      // 表单\n      formLogin: {\n        username: \"\",\n        password: \"\",\n        code: \"v9am\"\n      },\n      // 表单校验\n      rules: {\n        username: [\n          {\n            required: true,\n            message: \"请输入用户名\",\n            trigger: \"blur\"\n          }\n        ],\n        password: [\n          {\n            required: true,\n            message: \"请输入密码\",\n            trigger: \"blur\"\n          }\n        ],\n        code: [\n          {\n            required: true,\n            message: \"请输入验证码\",\n            trigger: \"blur\"\n          }\n        ]\n      }\n    };\n  },\n  mounted() {\n    this.timeInterval = setInterval(() => {\n      this.refreshTime();\n    }, 1000);\n  },\n  computed: {\n    user: function() {\n      return this.$store.state.d2admin.user.info.name;\n    }\n  },\n  beforeDestroy() {\n    clearInterval(this.timeInterval);\n  },\n  methods: {\n    refreshTime() {\n      this.time = dayjs().format(\"HH:mm:ss\");\n    },\n    /**\n     * @description 接收选择一个用户快速登录的事件\n     * @param {Object} user 用户信息\n     */\n    handleUserBtnClick(user) {\n      this.formLogin.username = user.username;\n      this.formLogin.password = user.password;\n      this.submit();\n    },\n    /**\n     * @description 提交表单\n     */\n    // 提交登录信息\n    submit() {\n      this.$refs.loginForm.validate(valid => {\n        if (valid) {\n          console.log(this.formLogin);\n          axios\n            .post(\n              \"/api/user/login?accountNumber=\" +\n                this.formLogin.username +\n                \"&password=\" +\n                this.formLogin.password\n            )\n            .then(Response => {\n              console.log(Response);\n              if (Response.status == 200) {\n                let userdata = Response.data;\n                this.$store.dispatch(\"d2admin/user/set\", {\n                  name: this.formLogin.username\n                });\n                axios\n                  .get(\"/api/user/query/accountNumber/\" + this.user)\n                  .then(function(res) {\n                    this.userid = res.data.data.id;\n                  });\n                this.$store.commit(\"setAccountnumber\", this.formLogin.username);\n\n                this.$router.push({ name: \"index\" });\n              } else {\n                this.$message.error(\"密码错误，请重新输入\");\n              }\n            })\n            .catch(error => {\n              this.$message.error(\"账号或密码错误，请重新输入\");\n            });\n        } else {\n          // 登录表单校验失败\n          this.$message.error(\"表单校验失败，请检查\");\n        }\n      });\n    },\n    // async register(){\n    //   const {data} = await axios.post('/api/user/register',{\n    //         \"accountNumber\" : \"221701418\",\n    //         \"password\" : \"nn584878\",\n    //         \"sex\":0,\n    //         \"name\":\"wsn\",\n    //         \"phoneNumber\":\"13225940608\",\n    //         \"head\":\"\",\n    //         \"address\":\"福州大学生活三区\",\n    //         \"info\":\"\"\n    //       })\n    //       console.log(data);\n    // }\n    async login() {\n      const { data } = axios.get(\"/api/user/login\", {\n        accountNumber: this.formLogin.username,\n        password: this.formLogin.password\n      });\n\n      console.log(data);\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.page-login {\n  @extend %unable-select;\n  $backgroundColor: #f0f2f5;\n  // ---\n  background-color: $backgroundColor;\n  height: 100%;\n  position: relative;\n  // 层\n  .page-login--layer {\n    @extend %full;\n    overflow: auto;\n  }\n  .page-login--layer-area {\n    overflow: hidden;\n  }\n  // 时间\n  .page-login--layer-time {\n    font-size: 24em;\n    font-weight: bold;\n    color: rgba(0, 0, 0, 0.03);\n    overflow: hidden;\n  }\n  // 登陆页面控件的容器\n  .page-login--content {\n    height: 100%;\n    min-height: 500px;\n  }\n  // header\n  .page-login--content-header {\n    padding: 1em 0;\n    .page-login--content-header-motto {\n      margin: 0px;\n      padding: 0px;\n      color: $color-text-normal;\n      text-align: center;\n      font-size: 12px;\n    }\n  }\n  // main\n  .page-login--logo {\n    width: 240px;\n    margin-bottom: 2em;\n    margin-top: -2em;\n  }\n  // 登录表单\n  .page-login--form {\n    width: 280px;\n    // 卡片\n    .el-card {\n      margin-bottom: 15px;\n    }\n    // 登录按钮\n    .button-login {\n      width: 100%;\n    }\n    // 输入框左边的图表区域缩窄\n    .el-input-group__prepend {\n      padding: 0px 14px;\n    }\n    .login-code {\n      height: 40px - 2px;\n      display: block;\n      margin: 0px -20px;\n      border-top-right-radius: 2px;\n      border-bottom-right-radius: 2px;\n    }\n    // 登陆选项\n    .page-login--options {\n      margin: 0px;\n      padding: 0px;\n      font-size: 14px;\n      color: $color-primary;\n      margin-bottom: 15px;\n      font-weight: bold;\n    }\n    .page-login--quick {\n      width: 100%;\n    }\n  }\n  // 快速选择用户面板\n  .page-login--quick-user {\n    @extend %flex-center-col;\n    padding: 10px 0px;\n    border-radius: 4px;\n    &:hover {\n      background-color: $color-bg;\n      i {\n        color: $color-text-normal;\n      }\n      span {\n        color: $color-text-normal;\n      }\n    }\n    i {\n      font-size: 36px;\n      color: $color-text-sub;\n    }\n    span {\n      font-size: 12px;\n      margin-top: 10px;\n      color: $color-text-sub;\n    }\n  }\n  // footer\n  .page-login--content-footer {\n    padding: 1em 0;\n    .page-login--content-footer-locales {\n      padding: 0px;\n      margin: 0px;\n      margin-bottom: 15px;\n      font-size: 12px;\n      line-height: 12px;\n      text-align: center;\n      color: $color-text-normal;\n      a {\n        color: $color-text-normal;\n        margin: 0 0.5em;\n        &:hover {\n          color: $color-text-main;\n        }\n      }\n    }\n    .page-login--content-footer-copyright {\n      padding: 0px;\n      margin: 0px;\n      margin-bottom: 10px;\n      font-size: 12px;\n      line-height: 12px;\n      text-align: center;\n      color: $color-text-normal;\n      a {\n        color: $color-text-normal;\n      }\n    }\n    .page-login--content-footer-options {\n      padding: 0px;\n      margin: 0px;\n      font-size: 12px;\n      line-height: 12px;\n      text-align: center;\n      a {\n        color: $color-text-normal;\n        margin: 0 1em;\n      }\n    }\n  }\n  // 背景\n  .circles {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    overflow: hidden;\n    margin: 0px;\n    padding: 0px;\n    li {\n      position: absolute;\n      display: block;\n      list-style: none;\n      width: 20px;\n      height: 20px;\n      background: #fff;\n      animation: animate 25s linear infinite;\n      bottom: -200px;\n      @keyframes animate {\n        0% {\n          transform: translateY(0) rotate(0deg);\n          opacity: 1;\n          border-radius: 0;\n        }\n        100% {\n          transform: translateY(-1000px) rotate(720deg);\n          opacity: 0;\n          border-radius: 50%;\n        }\n      }\n      &:nth-child(1) {\n        left: 15%;\n        width: 80px;\n        height: 80px;\n        animation-delay: 0s;\n      }\n      &:nth-child(2) {\n        left: 5%;\n        width: 20px;\n        height: 20px;\n        animation-delay: 2s;\n        animation-duration: 12s;\n      }\n      &:nth-child(3) {\n        left: 70%;\n        width: 20px;\n        height: 20px;\n        animation-delay: 4s;\n      }\n      &:nth-child(4) {\n        left: 40%;\n        width: 60px;\n        height: 60px;\n        animation-delay: 0s;\n        animation-duration: 18s;\n      }\n      &:nth-child(5) {\n        left: 65%;\n        width: 20px;\n        height: 20px;\n        animation-delay: 0s;\n      }\n      &:nth-child(6) {\n        left: 75%;\n        width: 150px;\n        height: 150px;\n        animation-delay: 3s;\n      }\n      &:nth-child(7) {\n        left: 35%;\n        width: 200px;\n        height: 200px;\n        animation-delay: 7s;\n      }\n      &:nth-child(8) {\n        left: 50%;\n        width: 25px;\n        height: 25px;\n        animation-delay: 15s;\n        animation-duration: 45s;\n      }\n      &:nth-child(9) {\n        left: 20%;\n        width: 15px;\n        height: 15px;\n        animation-delay: 2s;\n        animation-duration: 35s;\n      }\n      &:nth-child(10) {\n        left: 85%;\n        width: 150px;\n        height: 150px;\n        animation-delay: 0s;\n        animation-duration: 11s;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views/system/login"}]}